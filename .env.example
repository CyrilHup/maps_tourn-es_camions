# Environment Configuration
# Copy this file to .env.local and add your actual API keys

# Mapbox API Key (required for map functionality)
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# Google Maps API Key (alternative to Mapbox)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# HERE API Key (alternative routing service)
VITE_HERE_API_KEY=your_here_api_key_here

# OpenRouteService API Key (for real truck routing with driving-hgv profile)
# Sign up at: https://openrouteservice.org/dev/#/signup
# Free tier: 2,000 requests/day
VITE_OPENROUTESERVICE_API_KEY=your_openrouteservice_api_key_here

# Development settings
VITE_APP_ENV=development
VITE_API_BASE_URL=http://localhost:3001/api

# Feature flags
VITE_ENABLE_TRUCK_ROUTING=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_ANALYTICS=false
